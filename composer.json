{
  "name": "arafadev/multi-channel-notifications",
  "description": "Laravel package for unified multi-channel notifications across email, SMS, WhatsApp, Slack, Discord, Teams, Telegram, Messenger, and voice calls",
  "type": "library",
  "keywords": [
    "laravel",
    "notifications",
    "sms",
    "email",
    "whatsapp",
    "slack",
    "discord",
    "telegram",
    "teams",
    "messenger",
    "voice",
    "twilio",
    "multi-channel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ahmed Arafa",
      "email": "arafa.dev@gmail.com",
      "homepage": "https://github.com/arafadev"
    }
  ],
  "homepage": "https://github.com/arafadev/multi-channel-notifications",
  "support": {
    "issues": "https://github.com/arafadev/multi-channel-notifications/issues",
    "source": "https://github.com/arafadev/multi-channel-notifications"
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/mail": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/queue": "^9.0|^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.0",
    "twilio/sdk": "^7.0|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0|^11.0",
    "mockery/mockery": "^1.4",
    "laravel/pint": "^1.0",
    "phpstan/phpstan": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Arafa\\Notifications\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Arafa\\Notifications\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Arafa\\Notifications\\MultiChannelNotificationsServiceProvider"
      ],
      "aliases": {
        "Notify": "Arafa\\Notifications\\Facades\\Notify"
      }
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage",
    "format": "pint",
    "analyse": "phpstan analyse"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
